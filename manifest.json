{
  "manifest_version": 2,
  "name": "pure_tab",
  "version": "0.0.1",
  "description": "A pure new tab",
  "permissions": [
    "storage",
    "file://*"
  ],
  "requirements": {
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en"
}
